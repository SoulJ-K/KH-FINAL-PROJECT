<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.kh.bubblebee.member.model.vo.Member" alias="Member"/>
		<typeAlias type="com.kh.bubblebee.board.model.vo.Board" alias="Board"/>
		<typeAlias type="com.kh.bubblebee.board.model.vo.Review" alias="Review"/>
		<typeAlias type="com.kh.bubblebee.host.model.vo.Host" alias="Host"/>
		<typeAlias type="com.kh.bubblebee.notice.model.vo.Notice" alias="Notice"/>
		<typeAlias type = "com.kh.bubblebee.purchase.model.vo.Purchase" alias = "Purchase"/>
		<typeAlias type = "com.kh.bubblebee.purchase.model.vo.PBoard" alias = "PBoard"/>
		<typeAlias type = "com.kh.bubblebee.purchase.model.vo.PSList" alias = "PSList"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="mappers/member-mapper.xml"/>
		<mapper resource="mappers/board-mapper.xml"/>
		<mapper resource="mappers/notice-mapper.xml"/>
		<mapper resource="mappers/mypage-mapper.xml"/>
		<mapper resource="mappers/host-mapper.xml"/>
		<mapper resource = "mappers/purchase-mapper.xml"/>
	</mappers>
</configuration>
